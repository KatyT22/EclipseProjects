package $ProjectInvest.simulation;

import com.invest.entity.Investor;
import com.invest.entity.TableauDeBord;

/**
 * Hello world!
 *
 */
public class App 
{
    public static void main( String[] args )
    {
        System.out.println( "Entrez votre capital à investir" );
        int capital = Integer.parseInt(scanner.nextLine());
        
        Investor myInvestor = new Investor(capital);
        
        TableauDeBord monTableau = new TableauDeBord(myInvestor);
        
        monTableau.strategy(20, 6, 18, 12);
        myInvestor.getCapitalAInvestir();
    }
}
